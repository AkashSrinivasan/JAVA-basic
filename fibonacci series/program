
package basic;

public class Basic {

  
    public static void main(String[] args) {
       

          int a=0,b=1,c=0,i;
          
          int n=8;
          System.out.print(a+" "+b+" ");
          for(i=0;i<n;i++)
          {
              c=a+b;
              a=b;
              b=c;
              System.out.print(c+" ");
          }
    }
    
}
